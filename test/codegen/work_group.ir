; RUN: %tinytc-oc -O0 < %s | filecheck %s
func @t1() {
  %0 = constant 1.0 : f32
  %1 = work_group.reduce_add %0 : f32
; CHECK-LABEL: void t1({{.*}}
; CHECK:       float x1 = work_group_reduce_add(x);
}
func @t2() {
  %0 = constant [1.0, 0.0] : c32
  %1 = work_group.reduce_add %0 : c32
; CHECK-LABEL: void t2({{.*}}
; CHECK:       float2 x1 = (float2) (work_group_reduce_add(x.x), work_group_reduce_add(x.y));
}
