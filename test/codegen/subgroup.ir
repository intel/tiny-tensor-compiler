; RUN: %tinytc-oc -O0 < %s | filecheck %s
func @t1() {
    parallel {
        %0 = builtin.num_subgroups : i32
        %1 = builtin.subgroup_id : i32
        %2 = builtin.subgroup_local_id : i32
        %3 = builtin.subgroup_size : i32
    }
; CHECK:      int x = get_num_sub_groups();
; CHECK-NEXT: int x1 = get_sub_group_id();
; CHECK-NEXT: int x2 = get_sub_group_local_id();
; CHECK-NEXT: int x3 = get_sub_group_size();
}
